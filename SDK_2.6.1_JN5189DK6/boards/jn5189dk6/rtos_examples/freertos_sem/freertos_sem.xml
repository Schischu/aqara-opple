  <example id="jn5189dk6_freertos_sem" name="freertos_sem" category="rtos_examples" dependency="platform.drivers.clock.JN5189 middleware.amazon_freertos.freertos.heap_4.JN5189 middleware.amazon_freertos.freertos.JN5189 platform.drivers.common.JN5189 platform.drivers.power_no_lib.JN5189 device.JN5189_CMSIS.JN5189 device.JN5189_startup.JN5189 platform.drivers.flexcomm_usart.JN5189 platform.drivers.flexcomm.JN5189 platform.drivers.jn_iocon.JN5189 platform.drivers.lpc_gpio.JN5189 utility.debug_console.JN5189 platform.utilities.assert.JN5189 platform.drivers.reset.JN5189 platform.drivers.jn_romapi.JN5189 component.serial_manager.JN5189 component.serial_manager_uart.JN5189 component.lists.JN5189 component.usart_adapter.JN5189 platform.drivers.inputmux.JN5189 platform.drivers.lpc_adc.JN5189 platform.Include_core_cm4.JN5189 platform.Include_common.JN5189 platform.drivers.inputmux_connections.JN5189">
